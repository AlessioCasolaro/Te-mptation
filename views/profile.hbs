<div class="col-md-6 m-auto text-center" style="margin-top: 10rem!important;"> 
  <h2>Storico degli acquisti</h2>
  {{# each orders}}
  <div class="panel panel-default">
    <div class="panel-body mt-20">
      <!-- List group -->
     <ul class="list-group">
       {{#each this.items}}
        <li class="list-group-item">
          <span class="badge">{{ this.price}} €</span>
          {{ this.item.title }} | Quantità: {{this.qty}}
        </li>
       {{/each}}
     </ul>
    </div>
    <div class="panel-footer mt-10 mb-20">Totale ordine: {{ this.cart.totalPrice}}</div>
  </div>
  {{/each}}

</div>
